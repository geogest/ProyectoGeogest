        function RevisarSeleccionDePrestador(){
            var Compra = "PR";
            var Venta = "CL";

            var SelectTipoPrestador = document.getElementById("TipoPrestador");

            var CajaCheckCompra = document.getElementById("CajaContaCompra");
            var CajaCheckVenta = document.getElementById("CajaContaVenta");

            var CheckBoxCompra = document.getElementById("ContaLibroComra");
            var CheckBoxVenta = document.getElementById("ContaLibroVenta");

            if(SelectTipoPrestador != null){
                if(SelectTipoPrestador.value == Compra){
                    CajaCheckVenta.classList.add("Esconder");
                    CajaCheckCompra.classList.remove("Esconder");
                }else if(SelectTipoPrestador.value == Venta){
                    CajaCheckCompra.classList.add("Esconder");
                    CajaCheckVenta.classList.remove("Esconder");
                }
            }

            SelectTipoPrestador.addEventListener('change', function(){
                var SelectedOption = this.options[SelectTipoPrestador.selectedIndex];
                if(SelectedOption.value == Compra){
                    if(CheckBoxVenta != null && CheckBoxVenta.checked){
                        CheckBoxVenta.checked = 0;
                    }

                    CajaCheckVenta.classList.add("Esconder");
                    CajaCheckCompra.classList.remove("Esconder");
                }else if(SelectedOption.value == Venta){
                    if(CheckBoxCompra != null && CheckBoxCompra.checked){
                        CheckBoxCompra.checked = 0;
                    }
                    CajaCheckCompra.classList.add("Esconder");
                    CajaCheckVenta.classList.remove("Esconder");
                }else{
                    if(CheckBoxVenta != null && CheckBoxVenta.checked){
                        CheckBoxVenta.checked = 0;
                    }
                    if(CheckBoxCompra != null && CheckBoxCompra.checked){
                        CheckBoxCompra.checked = 0;
                    }
                    CajaCheckCompra.classList.add("Esconder");
                    CajaCheckVenta.classList.add("Esconder");
                }
            });
        }

