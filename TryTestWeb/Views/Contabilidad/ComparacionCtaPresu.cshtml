@{
    ViewBag.Title = "ComparacionCtaPresu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="clearfix"></div>
<br />

<div class="row">
    <div class="panel-heading">
        <div class="col-sm-12">


            <div class="pull-right">
                <br />
                <button class="btn btn-success waves-effect waves-light pull-right" value="Exportar" name="Save" type="submit" data-animation="fadein" data-plugin="custommodal" data-overlaySpeed="200" data-overlayColor="#36404a">
                    <span class="btn-label"><i class="fa fa-file-excel-o"></i></span>Exportar
                </button>
                <br />
            </div>
            <br />


            <h2 class="page-header">Comparación con Presupuesto</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="#">Contabilidad Reportes</a>
                </li>
                <li>
                    <a href="#">-</a>
                </li>
                <li class="active">
                    Informe Contabilidad
                </li>
            </ol>
        </div>
    </div>
</div>
<br />

<h2></h2>
<div class="container">
    <div class="row">
        
            <div class="col-md-6">
                <div class="card-box">
                    <h3 class="text-center">Totales Cuentas Contables</h3>
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th class="text-center">Codigo</th>
                                <th class="text-center">Nombre</th>
                                <th class="text-center">Total:</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (dynamic TablaCuentaCont in ViewBag.TablaCuentasContables)
                            {
                                @:<tr class="odd gradeX">
                                    for (int i = 0; i < TablaCuentaCont.Length; i++)
                                        {
                                            <td>@TablaCuentaCont[i]</td>
                                        }
                                @:</tr>
                           }
                        </tbody>
                    </table>
                </div>
            </div>
        
            <div class="col-md-6">
                <div class="card-box">
                   <h3 class="text-center">Totales Presupuesto</h3>
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th class="text-center">Codigo</th>
                                <th class="text-center">Nombre</th>
                                <th class="text-center">Total:</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (dynamic TablaPresupuesto in ViewBag.TablaPresupuesto)
                            {

                                @:<tr class="odd gradeX">
                                    for (int i = 0; i < TablaPresupuesto.Length; i++)
                                        {
                                            <td>@TablaPresupuesto[i]</td>
                                        }
                                @:</tr>
                            }
                        </tbody>
                    </table>

                </div>
         </div>

    </div>
</div>

<style>
    td{
        text-align:center;
    }
</style>