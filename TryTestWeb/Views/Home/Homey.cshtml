            <div class="row">
                <div class="col-sm-6 col-md-6">
                    @using (Html.BeginForm("SendCorreo", "Home", FormMethod.Post, new { id = "FormFactura" }))
                    {
                        <div class="row row-bottom-padded-md">
                            <div class="col-md-12"></div>
                            <div class="col-md-12 to-animate text-center">
                                <h3>Formulario</h3>
                                <div class="form-group ">
                                    <label for="nameContact" class="sr-only">Nombre Contacto</label>
                                    <input id="nameContact" name="nameContact" class="form-control" placeholder="Nombre Contacto" type="text">
                                </div>
                                <div class="form-group ">
                                    <label for="nameEmpresa" class="sr-only">Nombre Empresa</label>
                                    <input id="nameEmpresa" name="nameEmpresa" class="form-control" placeholder="Nombre Empresa" type="text">
                                </div>
                                <div class="form-group ">
                                    <label for="email" class="sr-only">Email</label>
                                    <input id="email" name="email" class="form-control" placeholder="Email" type="email">
                                </div>
                                <div class="form-group ">
                                    <label for="phone" class="sr-only">Teléfono</label>
                                    <input id="phone" name="phone" class="form-control" placeholder="Teléfono" type="text">
                                </div>
                                <div class="form-group ">
                                    <label for="message" class="sr-only">Mensaje</label>
                                    <textarea name="message" id="message" cols="30" rows="5" class="form-control" placeholder="Mensaje"></textarea>
                                </div>
                                <div class="form-group ">
                                    <div class="g-recaptcha" data-callback="recaptchaCallback" data-sitekey="6LeqAk8UAAAAAGX88c__hAA-oizzvtD6vRoY5Mix"></div>
                                </div>
                                <div class="form-group ">
                                    <input class="btn btn-primary btn-lg" value="Enviar" id="submitBtn" disabled type="submit">
                                </div>
                            </div>
                        </div>
                    }
