                    <p style="font-size:10px; color:gray;font-style:italic;">* Solo CAFs previamente cargados</p>
                    <label class="control-label" for="tipodoc">Tipo Documento: </label>
                    <select name="tipodoc" id="tipodoc" class="form-control required input-invalid" title="Seleccione el Tipo de documento">
                        <option value="">Seleccione tipo de Documento</option>
                        @foreach (SelectListItem tipoDocConCafCargados in lstValidDocs)
                        {
                            <option value="@tipoDocConCafCargados.Value">@tipoDocConCafCargados.Text</option>
                        }

                    </select>
                </div>
                <div class="row form-group">
                    <div class="col-md-6">
                        <label class="control-label">Fecha Emisión:</label>
                        <input id="datepicker-autoclose" class="form-control" name="EFXP_FCH_EMIS" type="text">
                    </div>
                    <div class="col-md-6">
                        <label class="control-label">Folio:</label>
                        <input id="EFXP_FOLIO" class="form-control" name="EFXP_FOLIO" type="number" readonly="readonly" placeholder="AUTO">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-md-offset-2" id="datosFact" style="display:none;">
                <h3 style="text-align: center;">Rut:<span id="Rut_Emisor">@Html.Raw(ParseExtensions.RutWithDots(Model.Item1.RUTEmpresa))</span></h3>
                <h3 style="text-align: center;"><span id="TIPO_DOC">[TIPO DOCUMENTO]</span></h3>
                <h3 style="text-align: center;">N°<span id="NumeroDoc">XX</span></h3>
            </div>
        </div>
    </div>

